<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Pantry</title>
  </head>
  <body>
    <%- include('../partials/nav.ejs') %>
    <h1>Welcome to my Pantry</h1>
    <a href="/foods/new">Add Item</a>

    <% pantry.forEach((foodItem)=> { %>
        <a href="/foods/<%= foodItem._id %>">
            <p><%= foodItem.name %></p>
        </a>
    <% }) %>
  </body>
</html>
